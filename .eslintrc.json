{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "react/jsx-one-expression-per-line": ["on", { "allow": "single-child" }],
    "react/destructuring-assignment": "off",
    "react/prop-types": "off",
    "jsx-a11y/label-has-for": "off"
  }
}